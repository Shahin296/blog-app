
 <%- include("partials/header"); -%>
   <h1>Home</h1>
  <p><%= startingContent %></p>
  <% posts.forEach(function(post){ %>
    <% const title = post.Title; %>
    <% const content = post.Content; %>
    <h1> <%= title %></h1>
    <p><%= content.substring(0,100) + " ..." %> <a href="/posts/<%=post._id%>">read more</a></p>
      
  <% }); %>
<%- include("partials/footer"); -%>




